{
  "name": "react-create-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.3.1",
    "@material-ui/icons": "^1.1.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.10",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "perfect-scrollbar": "^1.4.0",
    "promise.prototype.finally": "^3.1.0",
    "react": "^16.4.1",
    "react-app-rewire-mobx": "^1.0.8",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "5.0.1",
    "superagent": "^3.8.3",
    "superagent-promise": "^1.1.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "cross-env NODE_PATH=./src react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "server": "node_modules/.bin/json-server --watch server/db.json --port 8080 --routes server/routes.json"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "json-server": "^0.14.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "react-app-rewired": "^1.5.2"
  },
  "homepage": "http://zqqiang.github.io/nx-web"
}
